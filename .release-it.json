{
  "non-interactive": true,
  "git": {
    "requireUpstream": false
  },
  "src": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true,
    "tokenRef": "RELEASE_IT_GH_TOKEN",
    "assets": [ "dist/*.tar.gz" ]
  },
  "npm": {
    "publish": true
  },
  "dist": {
    "github": {
      "release": true,
      "tokenRef": "RELEASE_IT_GH_TOKEN",
      "assets": [ "./*" ]
    },
    "npm": {
      "publish": false
    }
  }
}
